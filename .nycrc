{
  "cache": false,
  "extension": [".ts", ".tsx"],
  "exclude": [
    "**/*.d.ts",
    "coverage/**",
    "packages/*/test/**",
    "test/**",
    "test{,-*}.ts",
    "**/*{.,-}{test,spec}.ts",
    "**/__tests__/**",
    "**/node_modules/**",
    "src/main.ts",
    "**/mock*.ts",
    "jest.config.js",
    "docs/**"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary"
  ]
}